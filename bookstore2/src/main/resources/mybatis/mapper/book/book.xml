<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.play.jcslib.bookstore2.mapper.BookMapper">
    <select id="getAllBooks" resultType="BookVo">
        SELECT  BOOK_ID,
                CAT_ID,
                BOOK_NM,
                AUTHOR_ID,
                PB_ID,
                PRICE,
                PB_DATE,
                BOOK_IMG,
                PAGES,
                VOLUME,
                BOOK_INTRO,
                RMK,
                RGR_ID,
                RGR_DTTI,
                CH_ID,
                CH_DTTI	
        FROM BOOK_MST
    </select>

    <!-- <insert id="insertBook" resultType="BookVo">
        SELECT BOOK_ID as id, 
               BOOK_NM as name
        FROM book_mst
    </insert> 테스트-->

</mapper>